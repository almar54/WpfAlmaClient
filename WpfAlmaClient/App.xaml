<Application x:Class="WpfAlmaClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfAlmaClient"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             StartupUri="CreateEventwnd.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="Primary" Color="#228EEE"/>
            <SolidColorBrush x:Key="Accent" Color="#68B8FF"/>
            <SolidColorBrush x:Key="RaisedLightButton" Color="#68B8FF"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
